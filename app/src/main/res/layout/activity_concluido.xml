<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    tools:context=".ConcluidoActivity">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="10dp"
        android:orientation="horizontal">

        <ImageView
            android:id="@+id/btnvoltar_concluido"
            android:layout_width="35dp"
            android:layout_height="35dp"
            app:srcCompat="?attr/actionModeCloseDrawable" />

        <TextView
            android:id="@+id/textView2"
            android:layout_width="244dp"
            android:layout_height="wrap_content"
            android:gravity="center"
            android:text="Concluidos"
            android:textColor="#DD070303"
            android:textSize="24sp" />

        <ImageView
            android:id="@+id/imgdeletar_concluido"
            android:layout_width="35dp"
            android:layout_height="35dp"
            android:layout_gravity="center"
            app:srcCompat="@mipmap/cancel" />
    </LinearLayout>

    <TextView
        android:id="@+id/txtstatus_concluido"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="20dp"
        android:layout_marginBottom="10dp"
        android:gravity="center"
        android:text="#status"
        android:textColor="#DD070303"
        android:textSize="18sp" />

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="10dp"
        android:orientation="horizontal"
        android:paddingTop="5dp">

        <TextView
            android:id="@+id/textView10"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="30dp"
            android:layout_weight="1"
            android:text="Carro:"
            android:textAllCaps="false"
            android:textColor="#DD070303"
            android:textSize="18sp"
            android:textStyle="bold" />

        <TextView
            android:id="@+id/txtcarro_concluido"
            android:layout_width="131dp"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:text="#status"
            android:textColor="#D3030101"
            android:textSize="18sp" />

    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal">

        <TextView
            android:id="@+id/textView11"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="30dp"
            android:layout_marginTop="5dp"
            android:layout_weight="1"
            android:text="Placa:"
            android:textAllCaps="false"
            android:textColor="#DD070303"
            android:textSize="18sp"
            android:textStyle="bold" />

        <TextView
            android:id="@+id/txtplaca_concluido"
            android:layout_width="133dp"
            android:layout_height="wrap_content"
            android:layout_marginTop="5dp"
            android:layout_weight="1"
            android:text="#status"
            android:textColor="#D3030101"
            android:textSize="18sp" />

    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal">

        <TextView
            android:id="@+id/textView17"
            android:layout_width="50dp"
            android:layout_height="wrap_content"
            android:layout_marginLeft="30dp"
            android:layout_marginTop="5dp"
            android:layout_weight="1"
            android:text="Lavagem:"
            android:textAllCaps="false"
            android:textColor="#DD070303"
            android:textSize="18sp"
            android:textStyle="bold" />

        <TextView
            android:id="@+id/txtlavagem_concluido"
            android:layout_width="134dp"
            android:layout_height="wrap_content"
            android:layout_marginTop="5dp"
            android:layout_weight="1"
            android:text="#status"
            android:textColor="#D3030101"
            android:textSize="18sp" />

    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal">

        <TextView
            android:id="@+id/textView21"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="30dp"
            android:layout_marginTop="5dp"
            android:layout_weight="1"
            android:text="Cliente:"
            android:textAllCaps="false"
            android:textColor="#DD070303"
            android:textSize="18sp"
            android:textStyle="bold" />

        <TextView
            android:id="@+id/txtcliente_concluido"
            android:layout_width="144dp"
            android:layout_height="wrap_content"
            android:layout_marginTop="5dp"
            android:layout_weight="1"
            android:text="#status"
            android:textColor="#D3030101"
            android:textSize="18sp" />

    </LinearLayout>

    <ImageView
        android:id="@+id/imageView"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="5dp"
        app:srcCompat="@drawable/lg1" />

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="57dp"
        android:layout_marginTop="10dp"
        android:orientation="horizontal">

        <ImageView
            android:id="@+id/imgprimeiro_concluido"
            android:layout_width="44dp"
            android:layout_height="57dp"
            android:layout_weight="1"
            app:srcCompat="@mipmap/botao_primeiro" />

        <ImageView
            android:id="@+id/imganterior_concluido"
            android:layout_width="44dp"
            android:layout_height="57dp"
            android:layout_weight="1"
            app:srcCompat="@mipmap/botao_anterior" />

        <ImageView
            android:id="@+id/imgproximo_concluido"
            android:layout_width="44dp"
            android:layout_height="57dp"
            android:layout_weight="1"
            app:srcCompat="@mipmap/botao_proximo" />

        <ImageView
            android:id="@+id/img_ultimo_concluido"
            android:layout_width="44dp"
            android:layout_height="57dp"
            android:layout_weight="1"
            app:srcCompat="@mipmap/botao_ultimo" />
    </LinearLayout>

</LinearLayout>